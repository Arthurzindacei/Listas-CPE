1. #include<iostream>
using namespace std;

float fahrenheit(int n){
        return (9.0/5.0) * n + 32.0;
    }

int main(){

    cout<< "A tabela a seguir possui todas as conversÃµes de celsius para fahrenheit (de 0C ate 100C)\n";

    for (int cel = 0; cel <=100; cel++){ 
        cout << cel << "C = " << fahrenheit(cel) << "F.\n";
    }
    return 0;
}
