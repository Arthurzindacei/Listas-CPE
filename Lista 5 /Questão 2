 #include<iostream>
using namespace std;

bool primo(int num){
    if (num <= 1)
        return false;
    for (int i = 2; i * i <= num; i++) { 
        if (num % i == 0) 
            return false;
    }
    return true; 
}

int main() {
    int num;
    cout<<"Digite um nÃºmero inteiro maior que 1:\n";
    cin >> num;

    if (primo(num))
        cout << num << " e um numero primo.\n";
    else
        cout << num << " nao e um numero primo.\n";
    return 0;
}
